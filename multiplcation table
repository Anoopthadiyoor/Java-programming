import java.util.*;
public class multi{
    public static void table(int num){
        for(int i =1; i<=10; i++){
            System.out.println(i+" * "+num+"="+(i*num));
        }
    }
public static void main(String[] args) {
    Scanner sc=new Scanner(System.in);
    System.out.println("enter a number:");
    int n= sc.nextInt();
    table(n);
}
}
    
